{
  "env": {"browser": true},
  "parser": "babel-eslint",
  "rules": {
    "semi": ["error", "always"],
    // "quotes": ["error", "double"],
    "react/prop-types": [0],
    "pretty-imports/sorted": "warn"
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins":[
    "react",
    "pretty-imports"
  ]
}